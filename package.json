{
  "name": "moodle-buddy",
  "version": "1.0.0",
  "author": "Marcel Reppenhagen",
  "scripts": {
    "start": "web-ext run -s ./build -f=nightly",
    "dev": "npm run nuke && cross-env NODE_ENV=development webpack --watch",
    "debug": "npm run nuke && cross-env NODE_ENV=debug webpack --watch",
    "build": "npm run nuke && cross-env NODE_ENV=production webpack",
    "nuke": "rm -rf build",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelreppi/moodle-buddy.git"
  },
  "bugs": {
    "url": "https://github.com/marcelreppi/moodle-buddy/issues"
  },
  "dependencies": {
    "sha.js": "^2.4.11",
    "vue": "^2.6.11",
    "vue-progressbar": "^0.7.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.5.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^5.0.2",
    "image-webpack-loader": "^6.0.0",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.3.0",
    "url-loader": "^3.0.0",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "web-ext": "^3.2.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
