{
  "name": "moodle-buddy",
  "version": "1.0.0",
  "author": "Marcel Reppenhagen",
  "scripts": {
    "dev": "parcel ./extension/popup/index.html --public-url . --out-dir ./extension/build",
    "start": "web-ext run -s ./extension",
    "build": "rm -rf ./extension/build && parcel build ./extension/popup/index.html --public-url . --out-dir ./extension/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelreppi/moodle-buddy.git"
  },
  "bugs": {
    "url": "https://github.com/marcelreppi/moodle-buddy/issues"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.4",
    "webextension-polyfill": "^0.5.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "vue-template-compiler": "^2.6.10",
    "web-ext": "^3.2.1"
  },
  "license": "MIT"
}
